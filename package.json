{
  "name": "react-component-boilerplate",
  "version": "1.0.0",
  "main": "src/MyComponent.js",
  "description": "Boilerplate for creating a React component. Includes hot-reloading.",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --compilers js:babel-core/register",
